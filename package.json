  "dependencies": {
    "antd": "3.25.2",
    "date-fns": "1.29.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "react-intl": "^2.9.0",
    "react-moment-proptypes": "^1.6.0",
    "react-redux": "^5.0.7",
    "react-resizable": "^1.7.5",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "react-split-pane": "^0.1.85",
    "react-tooltip": "^3.10.0",
    "redux": "^4.0.0",
    "redux-batched-actions": "^0.4.1",
    "redux-observable": "^1.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.4.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.11",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.8.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-plugin-react": "^7.11.1",
    "fs-jetpack": "^2.1.1",
    "gh-pages": "^3.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "inline-css": "^2.4.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "prop-types": "^15.6.2",
    "react": "16.4.2",
    "react-dom": "^16.4.2",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "scripts": {
    "test": "jest && node ./node_scripts/addCoverageReportStylesAsInline.js",
    "dev": "npm run clearDist && webpack --mode development",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "build": "npm run clearDist && webpack --mode production",
    "startDev": "webpack-dev-server --mode development --open",
    "startTest": "webpack-dev-server --mode development --port 8080 --hot --host 172.20.8.142 --open",
    "startProd": "webpack-dev-server --mode production --open",
    "start": "webpack-dev-server --mode development --open",
    "clearFolder": "node ./node_scripts/clearFolder.js",
    "clearDist": "npm run clearFolder ./dist",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./src && node ./node_modules/eslint/bin/eslint.js ./test",
    "updateTests": "jest -u --coverage",
    "clearJestCache": "jest --clearCache",
    "storybook": "start-storybook --port 8586"
  },
  "author": "Amol Naval",
  "license": "ISC",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.spec.js",
      "!src/index.js",
      "!src/state/*.js",
      "!src/store/*.js",
      "!src/test/*.js",
      "!src/reducers/rootReducers.js",
      "!src/epics/rootEpic.js"
    ],
    "coverageReporters": [
      "cobertura",
      "html",
      "text"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "browser": true,
    "moduleNameMapper": {
      "\\.(css|less|svg)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/test/enzymeSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "verbose": true
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "coverage/junit/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
